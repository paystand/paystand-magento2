{
    "name": "paystand/paystandmagento",
    "description": "Enables the PayStand payment method",
    "type": "magento2-module",
    "version": "3.4.0",
    "require": {
        "php": "~8.2.0",
        "ext-json": "*"
    },
    "require-dev": {
        "magento/magento2-functional-testing-framework": "^4.3.1",
        "phpunit/phpunit": "~9.5.0",
        "squizlabs/php_codesniffer": "~3.7.0",
        "magento/magento-coding-standard": "~5.0.0",
        "allure-framework/allure-phpunit": "~1.5.0",
        "magento/framework": "^103.0.7",
        "magento/module-backend": "^102.0.7",
        "magento/module-catalog": "^104.0.7",
        "magento/module-checkout": "^100.4.7",
        "magento/module-payment": "^100.4.7",
        "magento/module-quote": "^101.2.7",
        "magento/module-sales": "^103.0.7",
        "magento/module-store": "^101.1.7"
    },
    "suggest": {
        "magento/framework": "^103.0.7",
        "magento/module-backend": "^102.0.7",
        "magento/module-catalog": "^104.0.7",
        "magento/module-checkout": "^100.4.7",
        "magento/module-payment": "^100.4.7",
        "magento/module-quote": "^101.2.7",
        "magento/module-sales": "^103.0.7",
        "magento/module-store": "^101.1.7"
    },
    "license": [
        "Apache-2.0"
    ],
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "PayStand\\PayStandMagento\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PayStand\\PayStandMagento\\Test\\": "Test/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "magento/*": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    ]
}