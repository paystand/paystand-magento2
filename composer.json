{
    "name": "paystand/paystandmagento",
    "description": "Enables the PayStand payment method",
    "require": {
        "php": "~7.4.0||~8.1.0||~8.2.0||~8.3.0",
        "magento/module-sales": ">=100.0",
        "magento/module-store": ">=100.0",
        "magento/module-quote": ">=100.0",
        "magento/module-checkout": ">=100.0",
        "magento/module-backend": ">=100.0",
        "magento/module-payment": ">=100.0",
        "magento/module-catalog": ">=100.0",
        "magento/framework": ">=102.0.0 <104.0.0"
    },
    "type": "magento2-module",
    "version": "3.5.0",
    "license": [
        "Apache-2.0"
    ],
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "PayStand\\PayStandMagento\\": ""
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "magento/magento-coding-standard": "^32.0",
        "phpstan/phpstan": "^1.10"
    },
    "scripts": {
        "test": "phpunit",
        "phpcs": "phpcs --standard=Magento2 app/code/PayStand/PayStandMagento",
        "phpstan": "phpstan analyse app/code/PayStand/PayStandMagento"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/paystand/paystand-magento"
        }
    ]
}